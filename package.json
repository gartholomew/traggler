{
  "name": "traggler",
  "version": "0.1.0",
  "description": "An Electron (because why not) app for tagging and organizing MP3s.",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gartholomew/traggler.git"
  },
  "author": {
    "name": "James Brown",
    "email": "garthopotamus@gmail.com",
    "url": "https://github.com/gartholomew/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gartholomew/traggler/issues"
  },
  "homepage": "https://github.com/gartholomew/traggler#readme",
  "scripts": {
    "commit": "commit",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "main": "background.js",
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/prompt-cli": "^11.0.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "electron": "^9.0.0",
    "electron-devtools-installer": "^3.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-0",
    "husky": "^4.2.5",
    "vue-cli-plugin-electron-builder": "~2.0.0-rc.5"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
